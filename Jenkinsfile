pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh '''echo "Building"
echo "Build OK"'''
      }
    }
    stage('Test') {
      steps {
        sh '''echo "Testing"
echo "Test OK"'''
      }
    }
    stage('Deploy') {
      steps {
        sh '''echo "Deploying"
echo "Deploy OK"'''
      }
    }
  }
}